#include <iostream>
#include <vector>
#include <unordered_set>
using namespace std;

struct Node {
    int data;
    Node* next;
    Node(int v) {
        data = v;
        next = nullptr;
    }
};

bool hasCycle(Node* head) {
    unordered_set<Node*> s;
    Node* curr = head;
    while (curr) {
        if (s.find(curr) != s.end()) return true;
        s.insert(curr);
        curr = curr->next;
    }
    return false;
}

int main() {
    int n;
    cin >> n;
    if (n == 0) {
        cout << "false";
        return 0;
    }

    vector<Node*> nodes;
    nodes.reserve(n);
    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        nodes.push_back(new Node(x));
        if (i > 0) nodes[i - 1]->next = nodes[i];
    }

    int pos;
    cin >> pos;
    if (pos >= 0 && pos < n) {
        nodes[n - 1]->next = nodes[pos];
    }

    bool cycle = hasCycle(nodes[0]);
    if (cycle) cout << "true";
    else cout << "false";

    return 0;
}
